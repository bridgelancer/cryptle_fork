#!/usr/bin/awk -f

BEGIN {
    no = 0;
    nr = 0;
    na = 0;
}

/INFO/ && /Old/ && (/Buy/ || /Sell/) {
    no += 1;
    old[no] = $0;
}

/INFO/ && /RF/ && (/Buy/ || /Sell/) {
    nr += 1;
    rf[nr] = $0;
}

/INFO/ && /ATR/ && (/Buy/ || /Sell/) {
    na += 1;
    atr[na] = $0;
}

/INFO/ && /Old Cash/ {
    co = $7;
}

/INFO/ && /RF Cash/ {
    cr = $7;
}

/INFO/ && /ATR Cash/ {
    ca = $7;
}

END {
    print "========================"
    print "No. of Old trades: " no;
    print "No. of RF  trades: " nr;
    print "No. of ATR trades: " na;
    print ""
    print "Closing cash: " co;
    print "Closing cash: " cr;
    print "Closing cash: " ca;
    print "========================"
}
